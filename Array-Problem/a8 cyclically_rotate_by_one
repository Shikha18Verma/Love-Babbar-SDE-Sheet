#include<iostream>
using namespace std;

void rotate(int arr[], int n)
{
    int last = arr[n-1];
    for(int i = n-2; i>=0; i--){
        arr[i+1] = arr[i];
    }
    arr[0] = last;
    for(auto x: arr){
      cout<<x<<" "l 
    }
}

int main()
{
  int arr[] = {2,3,4,5,6,1};
  int n = sizeof(arr)/sizeof(int);
  rotate(arr, n);
  return 0;
}
